		<key name="WgNazwy" keyunique="true" keyprimary="true">
			<keycol name="Nazwa" />
		</key>
		<col name="Nazwa" type="string" length="30" required="true"/>
		<col name="Blokada" type="boolean"/>
    <col name="Opis" type="text"/>
  </table>

